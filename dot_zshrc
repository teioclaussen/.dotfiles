#####ALIASES#####
alias vi="vim"
alias cat="bat"
alias reload="source ~/.zshrc"
alias gen-ctl='python ~/git/proj-multiop/app_control/control.py'
alias find="find . -name"
alias zshconfig="vi ~/.zshrc"
alias codere="cd ~/git/proj-codere"
alias multiop="cd ~/git/proj-multiop"
alias sportium="cd ~/git/proj-sportium"
alias tdo="cd ~/git/proj-tdo"
alias mit="cd ~/git/proj-mit"

export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init --path)"

#####Navigation shortcuts#####
bindkey "[D" backward-word
bindkey "[C" forward-word
bindkey "^[a" beginning-of-line
bindkey "^[e" end-of-line
